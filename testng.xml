<suite name="Test Demo Run" verbose="10">
 <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
   </listeners>
  <sysproperty key="org.uncommons.reportng.escape-output" value="false" />    
  <parameter name="filenamePath" value="/Users/georgegaspar/Documents/Breakpoints.xml" />
  <parameter name="browser" value="*safari" />
  <parameter name="url" value="http://www.google.com" />
  <test name="Test Groups">
    <parameter name="browser" value="*safari" />
  	<parameter name="url" value="http://www.google.com" /> 
         <groups>      
         <run>
            <exclude name ="functest" />
            <exclude name ="checkintest" />
         </run>
         </groups>
     <classes>
    	<class name="utils.GroupTestExample"/>
  	</classes>  
  </test>
  <test name="Test on Safari" preserve-order="true">
  	<parameter name="browser" value="*safari" />
  	<parameter name="url" value="http://www.google.com" />
  	<classes>
  	      <class name="test.java.TestHboSignUp">
		     <methods>
        		<exclude name="testSignupFail" />
    		</methods>
      		</class>
	      <class name="test.java.TestPlayer">
	             <methods>
	        		<include name="testSignInSuccess" />
	        		<include name="testPlayerSuccess" />
	      		</methods>
	      		   	</class>
	      		   	<class name="test.java.TestHboSignIn">
	             <methods>
	        		<exclude name="testSignInSuccess" />
	        		<exclude name="testPlayerSuccess" />
	      		</methods>
	      		   	</class>
	    <class name="test.java.TestSearchEngines"> 
	          <methods>
	      		<exclude name="Yahoo" />
	      		<exclude name="Google" />
	    		</methods>
	    	</class>
	    <class name="utils.WebTestFactory" />   
   </classes>
  </test>
