<suite name="Test Demo Run" verbose="10">
 <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
   </listeners>
  <parameter name="browser" value="*firefox" />
  <parameter name="url" value="http://www.google.com" />
 
  <test name="TestTest" preserve-order="true">
    <classes>
      <class name="test.java.TestTest" />
      <class name="test.java.TestTest2" />
      <class name="test.java.WebTestFactory" />
     </classes>
  </test>
</suite>